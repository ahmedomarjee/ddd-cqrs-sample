context dddsample {

	namespace cargo {
		
		import dddsample.common.*
		import dddsample.cargo.*
		import dddsample.location.*
	
		aggregate Cargo {
			
			/** Uniquely identifies a particular cargo. */
			aggregate-id TrackingId base String {
				
				slabel "TID"
				label "Tracking ID"
				tooltip "Unique cargo identifier"
				
				/** Persistent value. */
				String number 
				
			}
			
		}
	
	
	}

}