context dddsample {

	namespace location {
		
		import dddsample.common.*
		import dddsample.location.*
		
		/**
		 * Name of a location.
		 */
		value-object LocationName base String {

			slabel "Name"
			label "Location name"
			tooltip "Unique name of the location"
			examples "Manhattan/New York" "Hamburg"
			
			/** Persistent value. */
			String name invariants Length(1, 100) 
			
		}
		
	}

}
