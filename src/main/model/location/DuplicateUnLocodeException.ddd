context dddsample {

	namespace location {
		
		import dddsample.location.Location.UnLocode
		
		/**
		 * The given UN location code already exists.
		 */
		exception DuplicateUnLocodeException cid 1001 {
			
			UnLocode code
			
			message "The given UN location code already exists: ${code}"
			
		}
	
	}
	
}
